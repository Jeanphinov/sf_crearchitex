{% block header %}
    <!DOCTYPE html>
    <html>
        <head>
            <meta charset="UTF-8" />
            <title>{% block title %}Crearchitex On Symfony 3!{% endblock %}</title>
<!--<link rel="stylesheet" href="{{ asset('css/*') }}" type="text/css"  />-->

            {% stylesheets 
            'css/960.css'
            'css/style.css'
            
            filter ='cssrewrite' %}    

            <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
            {% endstylesheets %}

            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        </head>
    {% endblock %}

    <body>
        <header class="largeur">
            <div class="container_12">
                <div class="logo"> 
                    {% image '@ISLCrearchitexBundle/Resources/images/logo.png' %}
                    <img src="{{ asset_url }}" alt="Logo Crearchitex" />
                    {% endimage %}
                </div>
                <nav>
                    <ul>
                        <li><a href="{{path('crearchitex_homepage')}}">home</a> </li>
                        <li><a href="{{path('crearchitex_equipe')}}">equipe</a></li>
                        <li><a href="{{path('crearchitex_projet')}}">projets</a></li>
                        <li><a href="{{path('crearchitex_news')}}">news</a></li>
                        <li><a href="{{path('crearchitex_contact')}}">contact</a></li>
                    </ul>
                </nav>
                <div class="clearfix"></div>
            </div>
        </header>
    {% block body %}{% endblock %}


