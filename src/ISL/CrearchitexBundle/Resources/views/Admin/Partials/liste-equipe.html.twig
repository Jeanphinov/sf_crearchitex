 <div class="content-box">
                    <div class="content-box-header">
                        <h3>Gestion des membres de l'équipe</h3>
                        <ul class="content-box-tabs">
                            <li><a href="?p=admin_equipe" class="default-tab current">Liste des entrées</a></li> <!-- href must be unique and match the id of target div -->
                            <li><a href="?p=admin_equipe&action=add">Ajouter</a></li>
                        </ul>
                        <div class="clear"></div>
                    </div>

                    <div class="content-box-content">
                        <div class="tab-content default-tab" id="tab1"> <!-- This is the target div. id must match the href of this div's tab -->
                                                        <table>
                                <thead>
                                    <tr>
                                        <th><input class="check-all" type="checkbox" /></th>
                                        <th>Nom</th>
                                        <th>Prénom</th>
                                        <th>Parcours / Description</th>
                                        <th class="alignright">Actions</th>
                                    </tr>
                                </thead>
                                
                                <tbody>
                                    {% for membre in equipe %}
                                                                                <tr>
                                                <td><input type="checkbox" name="groupe_id[]" value="3" /></td>
                                                <td>{{membre.nom}}</td>
                                                <td>{{membre.prenom}}</td>
                                                <td>{{membre.description}}</td>
                                                <td class="alignright">
                                                    <!-- Icons -->
                                                                                                            <!--// bouton de modification-->
                                                        <a href='{{path('crearchitex_equipe_modifier', {'id': membre.id})}}'title='Modifier'>
                                                            <img src='{{asset('images/icones/ico_update.png')}}' alt='modif' class='ico_action' />
                                                                 
                                                        </a>
                                                        <!--// bouton de suppression-->
                                                        <a href='{{path('crearchitex_equipe_supprimer', {'id': membre.id})}}' title='Supprimer'>
                                                            <img src='{{asset('images/icones/ico_delete.png')}}' alt='supprimer' class='ico_action' />
                                                        </a>
                                                                                                        </td>
                                            </tr>
                                            {% endfor %}
                                            
                                                                                
                                </tbody>

                            </table>
                            
                        </div> <!-- End #tab1 -->       

                    </div> <!-- End .content-box-content -->

                </div> <!-- End .content-box -->
                                <div class="clear"></div>
               